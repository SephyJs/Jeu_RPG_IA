# Copier ce fichier vers .env puis ajuster les valeurs si besoin.
# Exemple: cp .env.example .env

# Modele pour les regles JSON et calculs systeme
ATARYXIA_RULES_MODEL_KEY=mistral

# Modele pour les dialogues PNJ
ATARYXIA_DIALOGUE_MODEL_KEY=dolphin

# Modele pour la narration
ATARYXIA_NARRATION_MODEL_KEY=dolphin

# Valeurs supportees actuellement:
# - mistral
# - dolphin
# - qwen

# Token du bot Telegram (obligatoire pour app.telegram.bot)
TELEGRAM_BOT_TOKEN=

# Slots Telegram (optionnel)
TELEGRAM_DEFAULT_SLOT=1
TELEGRAM_SLOT_COUNT=3

# Optionnel: partager la meme sauvegarde que l'UI web (ex: sephy)
# Si vide, le bot utilise un profil isole par chat Telegram.
TELEGRAM_PROFILE_KEY=
TELEGRAM_PROFILE_NAME=
