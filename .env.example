# Copier ce fichier vers .env puis ajuster les valeurs si besoin.
# Exemple: cp .env.example .env

# Modele pour les regles JSON et calculs systeme
ATARYXIA_RULES_MODEL_KEY=mistral

# Modele pour les dialogues PNJ
ATARYXIA_DIALOGUE_MODEL_KEY=dolphin

# Modele pour la narration
ATARYXIA_NARRATION_MODEL_KEY=dolphin

# Modele Telegram Ataryxia (conversation SMS)
ATARYXIA_TELEGRAM_MODEL_KEY=dolphin
# 1 = pas de fallback automatique sur d'autres modeles
ATARYXIA_TELEGRAM_DISABLE_FALLBACK=0
ATARYXIA_TELEGRAM_TEMP=0.48
ATARYXIA_TELEGRAM_RETRY_TEMP=0.38
ATARYXIA_TELEGRAM_NUM_PREDICT=240
ATARYXIA_TELEGRAM_RETRY_NUM_PREDICT=240

# Valeurs supportees actuellement:
# - mistral
# - dolphin
# - qwen

# Token du bot Telegram (obligatoire pour app.telegram.bot)
TELEGRAM_BOT_TOKEN=

# Slots Telegram (optionnel)
TELEGRAM_DEFAULT_SLOT=1
TELEGRAM_SLOT_COUNT=3

# Optionnel: partager la meme sauvegarde que l'UI web (ex: sephy)
# Si vide, le bot utilise un profil isole par chat Telegram.
TELEGRAM_PROFILE_KEY=
TELEGRAM_PROFILE_NAME=

# Optionnel mais recommande: secret pour chiffrer les tokens Telegram stockes localement.
# Si vide, une cle locale est generee dans saves/.telegram_token_secret.
ATARYXIA_TELEGRAM_TOKEN_SECRET=

# Verrou du Mode Adulte (UI web)
# Definir soit le mot de passe en clair:
ATARYXIA_NSFW_PASSWORD=
# ... soit son SHA256 (hexadecimal lowercase):
ATARYXIA_NSFW_PASSWORD_SHA256=
